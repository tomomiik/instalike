<%= form_for(@instalike, url: choose_new_or_edit) do |f| %>
<%= f.error_messages! %>
<div class="imageupload">
 <%= f.label :image %>
 <%= f.file_field :image %>
</div>
 <br>
 <%= f.label :title %>
 <%= f.text_field :title, class: 'form_control' %>
 <br>
 <%= f.label :content %>
 <%= f.text_field :content, class: 'form_control' %>
 <br>
 <br>
 <%= f.submit 'POST!', class: 'btn btn-primary btn-block' %>
<% end %>
 <br>
<%= link_to "Back To Pictures List", instalikes_path %>
